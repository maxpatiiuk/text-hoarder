{
  "name": "Text Hoarder",
  "short_name": "Text Hoarder",
  "version": "0.0.1",
  "description": "TODO: title",
  "author": "Max Patiiuk",
  "manifest_version": 3,
  "icons": {
    "16": "./public/images/extension_icon_16x16.png",
    "32": "./public/images/extension_icon_32x32.png",
    "48": "./public/images/extension_icon_48x48.png",
    "128": "./public/images/extension_icon_128x128.png",
    "1024": "./public/images/extension_icon_1024x1024.png"
  },
  "key": "",
  "current_locale": "en",
  "permissions": ["identity", "storage", "activeTab", "scripting"],
  "optional_host_permissions": ["https://*/*", "http://*/*"],
  "background": {
    "service_worker": "./dist/background.bundle.js"
  },
  "cross_origin_embedder_policy": {
    "value": "require-corp"
  },
  "cross_origin_opener_policy": {
    "value": "same-origin"
  },
  "options_page": "./index.html",
  "action": {
    "default_title": "Open Reader Mode"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "mac": "Command+E",
        "default": "Ctrl+E"
      },
      "description": "Open Reader Mode"
    },
    "saveText": {
      "suggested_key": {
        "mac": "Command+Shift+E",
        "default": "Ctrl+Shift+E"
      },
      "description": "Save current page to GitHub repository (while extension is active)"
    },
    "editText": {
      "description": "Open Editor for current page in GitHub repository (while extension is active)"
    },
    "download": {
      "description": "Download current webpage (while extension is active)"
    }
  }
}
